<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <!-- Sequence for Orthanc Order -->
    <record id="seq_orthanc_order" model="ir.sequence">
        <field name="name">Orthanc Order</field>
        <field name="code">orthanc.order</field>
        <field name="implementation">no_gap</field>
        <field name="prefix">ORD-</field>
        <field name="padding">5</field>
        <field name="company_id" eval="False"/>
    </record>

    <!-- Sequence for Patient ID -->
    <record id="seq_patient_id" model="ir.sequence">
        <field name="name">Patient ID</field>
        <field name="code">abershum.patient.id.sequence</field>
        <field name="implementation">no_gap</field>
        <field name="prefix">PAT-</field>
        <field name="padding">6</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>
